<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="55dp"
    android:gravity="center"
    android:elevation="12dp"
    android:layout_marginTop="55dp"
    android:background="@drawable/navbar_bg">

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/tourists"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="42dp"
        android:layout_marginTop="6dp"
        android:backgroundTint="@android:color/transparent"
        android:outlineProvider="none"
        android:src="@drawable/faves"
        app:borderWidth="0dp"
        app:fabSize="mini"
        app:rippleColor="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:tint="@color/black_custom" />
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/terminals"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="6dp"
        android:src="@drawable/terminal_navbar"
        app:borderWidth="0dp"
        app:fabSize="mini"
        app:rippleColor="@color/white"
        app:tint="@color/black_custom"
        android:outlineProvider="none"
        android:backgroundTint="@android:color/transparent"
        app:layout_constraintStart_toEndOf="@+id/beaches"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/restos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="6dp"
        android:src="@drawable/restaurant_navbar"
        app:borderWidth="0dp"
        app:fabSize="mini"
        app:rippleColor="@color/white"
        android:outlineProvider="none"
        app:tint="@color/black_custom"
        android:backgroundTint="@android:color/transparent"
        app:layout_constraintStart_toEndOf="@+id/bldgs"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/bldgs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="6dp"
        android:src="@drawable/bldg_navbar"
        app:borderWidth="0dp"
        app:fabSize="mini"
        app:rippleColor="@color/white"
        android:outlineProvider="none"
        app:tint="@color/black_custom"
        android:backgroundTint="@android:color/transparent"
        app:layout_constraintStart_toEndOf="@+id/tourists"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/beaches"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="6dp"
        android:src="@drawable/beach_navbar"
        app:borderWidth="0dp"
        app:rippleColor="@color/white"
        app:fabSize="mini"
        android:outlineProvider="none"
        app:tint="@color/black_custom"
        android:backgroundTint="@android:color/transparent"
        app:layout_constraintStart_toEndOf="@+id/restos"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>