<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ca918c8c"/>
            <corners
                android:topLeftRadius="30dp"
                android:topRightRadius="30dp"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="#EDAC1A"/>
            <padding
                android:left="0dp"
                android:right="0dp"
                android:bottom="0dp"
                android:top="0dp"/>
            <corners
                android:topLeftRadius="30dp"
                android:topRightRadius="30dp"/>
        </shape>
    </item>
</layer-list>